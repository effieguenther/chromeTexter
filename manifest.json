{
"manifest_version": 3,
"name": "Chrome Texter",
"version": "1.0",
"author": "effieguenther",

"action": {
    "default_icon": {
        "30": "images/icons8-font-size-30.png"
    },
    "default_popup": "popup.html",
    "scripts": ["popup.js"],
    "default_title": "Chrome Texter"
},

"description": "Converts text formatting in various ways",

"permissions": [
    "activeTab", "scripting"
],

"background": {
    "service_worker": "background.js"
}

}